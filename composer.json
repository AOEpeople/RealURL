{
    "name": "aoepeople/realurl",
    "type": "typo3-cms-extension",
    "description": "Fork of EXT:realurl with the AOE patchset applied",
    "homepage": "https://github.com/AOEpeople/realurl",
    "support": {
        "issues": "https://github.com/AOEpeople/realurl/issues"
    },
    "version": "1.12.8.13",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "AOEpeople",
            "email": "dev@aoe.com",
            "homepage": "https://www.aoe.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms": "^8.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "nimut/testing-framework": "*"
    },
    "replace": {
        "realurl":  "self.version",
        "typo3-ter/realurl":  "self.version"
    },
    "autoload": {
        "classmap": [
            "modfunc1",
            "Classes/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "Tests/"
        ],
        "psr-4": {
            "AOE\\Realurl\\": "Classes/"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/realurl ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/realurl"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.12.x-dev"
        },
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        },
        "typo3/class-alias-loader": {
            "class-alias-maps": [
                "Migrations/Code/ClassAliasMap.php"
            ]
        }
    }
}
